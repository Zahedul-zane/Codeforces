
#include <bits/stdc++.h>
using namespace std;
 
int main() {
	int t;
    cin>>t;
    while(t--){
    int n;
    string s;
    cin>>n>>s;
    int cost2026 = 4;
    for (int i = 0; i <= n - 4; ++i) {
        int diff = (s[i]!='2') + (s[i+1]!='0') + (s[i+2]!='2') + (s[i+3]!='6');
        cost2026 = min(cost2026, diff);
    }
 
    int costNo2025 = 0;
    string temp = s;
    size_t pos = temp.find("2025");
    while (pos != string::npos) {
        costNo2025++;
        temp[pos + 3] = '#'; 
        pos = temp.find("2025", pos + 1);
    }
 
    cout << min(cost2026, costNo2025) << endl;
}
    }
